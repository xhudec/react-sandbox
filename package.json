{
  "name": "react-sandbox",
  "version": "0.1.0",
  "author": {
    "name": "Lukas Hudec",
    "email": "hudec.luke@gmail.com"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "tslint --config tslint.yaml --project tsconfig.json 'src/**/*.ts{,x}'",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|less|graphql)\"",
    "prettier:format": "npm run prettier -- --write",
    "prettier:validate": "npm run prettier -- --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:static": "npm audit && npm run prettier:validate && npm run lint",
    "dependencies:upgrade": "npm-check --update",
    "cypress:open": "cypress open"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "styled-components": "^4.3.0",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/node": "^10.12.21",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/styled-components": "^4.1.16",
    "@types/uuid": "^3.4.4",
    "cypress": "^3.3.1",
    "husky": "^2.4.0",
    "is-ci-cli": "^1.1.1",
    "jest-dom": "^3.4.0",
    "lint-staged": "^8.2.0",
    "npm-check": "^5.9.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1"
  }
}
